{
  "name": "@kppk/source",
  "version": "0.0.0",
  "scripts": {
    "update:npm:check": "ncu",
    "update:npm:install": "ncu -u",
    "nx:migration:latest": "npx nx migrate latest",
    "nx:migration:run": "nx migrate --run-migrations --verbose",
    "update:browser": "npx update-browserslist-db@latest",
    "kppk-react-mf:host:build:production": "nx run react-host:build --configuration=production --parallel=true",
    "kppk-react-mf:host:build:development": "nx run react-host:build --configuration=development --parallel=true",
    "kppk-react-mf:host:serve": "nx run react-host:serve:all",
    "kppk-react-mf:host:serve:all": "nx run react-host:serve --configuration=development --devRemotes=react_view_login,react_view_home,react_view_calc",
    "kppk-react-mf:host:serve:login": "nx run react-host:serve --configuration=development --devRemotes=react_view_login --parallel=true --hmr=true",
    "kppk-react-mf:host:serve:home": "nx run react-host:serve --configuration=development --devRemotes=react_view_home --parallel=true --hmr=true",
    "kppk-react-mf:host:serve:calc": "nx run react-host:serve --configuration=development --devRemotes=react_view_calc --parallel=true --hmr=true",
    "kppk-react-mf:host:test": "nx run react-host:test --parallel=true --maxWorkers=75% --bail=5 --watchAll=true",
    "kppk-react-mf:login:serve": "nx run react_view_login:serve --hmr=true --parallel=true --maxParallel=75% --allowedHosts=react-host",
    "kppk-react-mf:home:serve": "nx run react_view_home:serve --hmr=true --parallel=true --maxParallel=75% --allowedHosts=react-host",
    "kppk-react-mf:calc:serve": "nx run react_view_calc:serve --hmr=true --parallel=true --maxParallel=75% --allowedHosts=react-host",
    "react-lib:test": "nx run react-lib:test --parallel=true --maxWorkers=75% --bail=5 --watchAll=true",
    "react-lib:test-file": "nx run react-lib:test --runInBand=true --bail=5 --watch=true --testFile='src/lib/machine/react-init/test.spec.ts'",
    "react-lib-vite:test": "nx run react-lib-vite:test --parallel=true --maxParallel=75% --watch=true --bail=5",
    "react-lib-vite:test:file": "nx run react-lib-vite:test --parallel=true --maxParallel=75% --watch=true --testFile='src/lib/machine/fg-spinner/fg-spinner.machine.actor.service.spec.ts'",
    "kppk-react:serve": "nx run kppk-react:serve --parallel=true --maxParallel=75% --watch=true --hmr=true",
    "kppk-react:auth-local": "tsx ./scripts/fg-auth-local/fg-auth-local.script.ts ./apps/kppk-react/src/fg-auth-local-users.json ./../../apps/kppk-react/public/auth-local/",
    "start": "npm run kppk-react:serve"
  },
